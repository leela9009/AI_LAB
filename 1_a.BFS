def append(l1,l2):
    for i in l2:
        if i not in l1:
            l1.append(i)
    return l1

def BFS(g, root, goal):
    open = [root]
    closed = []
    while(goal not in closed):
        x = open.pop(0)
        if x in g.keys():
            open = Append(open, g[x])
        closed.append(x)
        print(open, closed)
        
